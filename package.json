{
  "name": "scaffolding-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3001",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:e2e": "playwright test",
    "audit:seo": "node scripts/seo-audit.mjs https://leegoscaffolding.com",
    "validate:seo": "node scripts/validate-seo.mjs",
    "check:performance": "node scripts/performance-check.mjs",
    "analyze:bundle": "node scripts/analyze-bundle.mjs",
    "check:bundle": "npm run build && ls -lh .next/static/chunks/*.js | awk '{print $5, $9}' | sort -rh | head -10",
    "build:seo": "npm run validate:seo && npm run check:performance && npm run build",
    "optimize": "npm run analyze:bundle && npm run build",
    "build:analyze": "npm run build && npm run analyze:bundle"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "json-server": "^1.0.0-beta.3",
    "lucide-react": "^0.546.0",
    "next": "^16.1.4",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "^16.1.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
